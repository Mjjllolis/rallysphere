{
  "expo": {
    "name": "RallySphere",
    "slug": "rally-sphere",
    "scheme": "rallysphere",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Logo.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/Logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "experiments": {
      "typedRoutes": true
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rallysphere.app",
      "buildNumber": "2"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.rallysphere.app",
      "versionCode": 2
    },
    "web": {
      "favicon": "./assets/Logo.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-secure-store",
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "merchant.com.rallysphere.app",
          "enableGooglePay": true
        }
      ]
    ],
    "owner": "ibemisha",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ee1b7bed-c9c2-4c0f-aee6-43e55c75b4d9"
      }
    }
  }
}
